    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "awesome-typescript-loader": "^5.2.1",
    "axe-core": "^4.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
