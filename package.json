    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npm run lin:fix"
    ],
    "cypress/**/*.{js,ts}": [
      "cd cypress && npm run lint:fix"
    ]
  }
}
